<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<shortcuts>

  <programGroup defaultName="All Purpose MD5" location="applications"/>
  <skipIfNotSupported/> 
  <programGroup defaultName="All Purpose MD5" location="applications"/>
  
  <shortcut
     name="All Purpose MD5"
     programGroup="yes"
     desktop="yes"
     applications="no"
     startMenu="yes"
     startup="no"
     description="All Purpose MD5"
     workingDirectory="$INSTALL_PATH" 
     target="java" 
     commandLine="-jar &quot;$INSTALL_PATH/APMD5.jar&quot;"
     iconFile="$INSTALL_PATH/images/checksum.png"
     iconIndex="0"
     type="Application"
     encoding="UTF-8"
     createForAll="true" 
     initialState="normal">
     
     <createForPack name="Application"/>
   </shortcut>

	<!--workingDirectory="&quot;$INSTALL_PATH&quot;"      
	target="$INSTALL_PATH/apmd5.sh" -->

   <shortcut
     name="All Purpose MD5 Uninstaller"
     programGroup="yes"
     desktop="no"
     applications="no"
     startMenu="yes"
     startup="no"
     target="java"
     commandLine="-jar &quot;$INSTALL_PATH/Uninstaller/uninstaller.jar&quot;"
     initialState="noShow"
     iconFile="trashcan_full" 
     iconIndex="0" 
     workingDirectory=""
     type="Application"
     encoding="UTF-8"
     createForAll="false"
     description="This uninstalls All Purpose MD5">
     <createForPack name="Application" />
   </shortcut>

<!--      iconFile="trashcan_full" -->
</shortcuts>
