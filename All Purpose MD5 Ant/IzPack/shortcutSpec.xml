<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<shortcuts>
  <skipIfNotSupported />
  
  <programGroup defaultName="All Purpose MD5"
                location="applications"/>
	<shortcut
		name="All Purpose MD5"
		target="$INSTALL_PATH\apmd5.jar"
		commandLine=""
		workingDirectory="$INSTALL_PATH"
		description="This starts All Purpose MD5"
		iconFile="$INSTALL_PATH\images\apmd5.ico"
		iconIndex="0"
		initialState="normal"
		programGroup="yes"
		desktop="yes"
		applications="no"
		startMenu="no"
		startup="no">
		<createForPack name="Application"/>
	</shortcut>
  
	<shortcut
		name="All Purpose MD5 Uninstaller"
		programGroup="yes"
		desktop="no"
		applications="no"
		startMenu="no"
		startup="no"
		target="$INSTALL_PATH\Uninstaller\uninstaller.jar"
		commandLine=""
		iconFile="%SystemRoot%\system32\SHELL32.dll"
		iconIndex="31"
		description="This uninstalls All Purpose MD5">
		<createForPack name="Application"/>
	</shortcut>
	
</shortcuts>
